{
  "name": "error-recorder",
  "displayName": "error-recorder",
  "description": "Recorder for error",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:errorRecorder-sidebar",
    "onCommand:error-recorder.postError",
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "errorRecorder-sidebar-view",
          "title": "ErrorRecorder",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "vsinder-sidebar-view": [
        {
          "type": "webview",
          "id": "errorRecorder-sidebar",
          "name": "ErrorRecorder",
          "icon": "media/checklist.svg",
          "contextualTitle": "ErrorRecorder"
        }
      ],
      "explorer": [
        {
          "id": "nodeDependencies",
          "name": "ErrorRecorder",
          "when": "workspaceHasPackageJSON",
          "icon": "media/dep.svg",
          "contextualTitle": "Package Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "error-recorder.errorRecorder",
        "title": "errorRecorder"
      },
      {
        "command": "error-recorder.postError",
        "title": "postErrorCode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
