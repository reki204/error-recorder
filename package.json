{
  "name": "error-recorder",
  "displayName": "error-recorder",
  "description": "Recorder for error",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:errorRecorderSidebar",
    "onCommand:error-recorder.index",
    "onCommand:error-recorder.postError",
    "onCommand:error-recorder.saveStorage"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "errorRecorder-sidebar-view",
          "title": "ErrorRecorder",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "errorRecorder-sidebar-view": [
        {
          "type": "webview",
          "id": "errorRecorderSidebar",
          "name": "ErrorRecorder",
          "icon": "media/checklist.svg",
          "contextualTitle": "ErrorRecorder"
        }
      ]
    },
    "commands": [
      {
        "command": "error-recorder.errorRecorder",
        "title": "errorRecorder"
      },
      {
        "command": "errorRecorder.index",
        "title": "errorRecorderIndex"
      },
      {
        "command": "error-recorder.postError",
        "title": "postErrorCode"
      },
      {
        "command": "error-recorder.saveStorage",
        "title": "saveStorage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.54",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.22.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.1.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.3.0",
    "uuid": "^9.0.0"
  }
}
